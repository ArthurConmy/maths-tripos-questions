<h4>Current</h4>
{% for course in collections.ibCurrent %}
  {% set courseUrl %}/courses/{{ course }}/{% endset %}

  <a href="{{ courseUrl | url }}" class="post-tag">{{ course }}</a>

{% endfor %}
<h4>Discontinued</h4>
{% for course in collections.ibOld %}
  {% set courseUrl %}/courses/{{ course }}/{% endset %}

  <a href="{{ courseUrl | url }}" class="post-tag">{{ course }}</a>

{% endfor %}