<ul class="post-list">
  {% for post in postslist %}
    <li class="post-list__item">
      <div>
        <div class="post-list__tags">
          {% set courseUrl %}/course/{{ post.data.course }}/{% endset %}
          <a href="{{ courseUrl | url }}">#{{ post.data.course }}</a>
        </div>
      </div>

      <h3 class="post-list__title">
        <a  href="{{ post.url }}">{{ post.data.title | md | safe }}</a>
      </h3>

      <!-- <p class="post-list__excerpt">{{ post.templateContent | md | safe  }}</p> -->
    </li>
  {% endfor %}
</ul>